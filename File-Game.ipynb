         return 44677
     score = game()
     with open("Hiscore.txt") as f:
         hiscoreStr = f.read()
    
     if  hiscoreStr=='' :
         with open("Hiscore.txt","w") as f:
             f.write(str(score)) 
        
elif int(hiscoreStr)<score :
    with open("Hiscore.txt","w") as f:
        f.write(str(score))
